.flip-card {
  perspective: 1000px;
}

.flip-inner {
  position: relative;
  width: 100%;
  height: 100%;
  transition: transform 0.6s;
  transform-style: preserve-3d;
}

.flip-card:hover .flip-inner {
  transform: rotateY(180deg);
}

.flip-front, .flip-back {
  position: absolute;
  width: 100%;
  height: 100%;
  backface-visibility: hidden;
  top: 0;
  left: 0;
  border-radius: 10px;
  padding: 20px;
  box-sizing: border-box;
  background: white;
}

.flip-back {
  transform: rotateY(180deg);
}
.face.back {
  padding: 0;
  margin: 0;
  display: flex;
  align-items: stretch;
  justify-content: center;
}

.card {
  width: 100%; /* or your fixed width */
