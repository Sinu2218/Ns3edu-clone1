<script>
  const ns3CardWrapper = document.getElementById("ns3CardWrapper");
  const ns3CardWidth = 250;
  const ns3CardsPerView = 4;
  let ns3ScrollIndex = 0;

  function ns3ScrollCards(direction) {
    const totalCards = ns3CardWrapper.children.length;
    const maxIndex = Math.ceil(totalCards / ns3CardsPerView) - 1;

    ns3ScrollIndex += direction;
    if (ns3ScrollIndex < 0) ns3ScrollIndex = 0;
    if (ns3ScrollIndex > maxIndex) ns3ScrollIndex = maxIndex;

    ns3CardWrapper.style.transform = `translateX(-${ns3ScrollIndex * ns3CardWidth * ns3CardsPerView}px)`;
  }
</script>
