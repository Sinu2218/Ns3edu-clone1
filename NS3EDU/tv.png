<script>
    // FAQs JS

    let question = document.querySelectorAll(".question");
    const viewMoreBtn = document.getElementById("view-more-faq-btn");
    let isExpanded = false; // Flag to track expanded state

    question.forEach(question => {
        question.addEventListener("click", event => {
            const active = document.querySelector(".question.active");
            if (active && active !== question) {
                active.classList.toggle("active");
                active.nextElementSibling.style.maxHeight = 0;
            }
            question.classList.toggle("active");
            const answer = question.nextElementSibling;
            if (question.classList.contains("active")) {
                answer.style.maxHeight = answer.scrollHeight + "px";
            } else {
                answer.style.maxHeight = 0;
            }
        })
    })
    viewMoreBtn.addEventListener("click", () => {
        const hiddenFAQs = document.querySelectorAll(".initially-hidden");
        hiddenFAQs.forEach(faq => {
            faq.style.display = isExpanded ? "none" : "block";
        });

        viewMoreBtn.textContent = isExpanded ? "View More" : "View Less";
        isExpanded = !isExpanded;
    });
</script>

