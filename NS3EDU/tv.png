</script>

<script>
    document.addEventListener('DOMContentLoaded', function() {
    const allCoursesButton = document.getElementById('all-courses');
    const dropdownContainer = document.getElementById('dropdown-container');

    // Show the first category's course list by default
    const defaultCategory = 'networking-courses';
    document.getElementById(defaultCategory).classList.remove('d-none');
    document.getElementById(defaultCategory).classList.add('d-block');

    // Add active class to the default category link
    const defaultCategoryLink = document.querySelector(`[data-target="${defaultCategory}"]`);
    defaultCategoryLink.classList.add('active');

    // Event listener for hovering over category links
    document.querySelectorAll('.category-submenu-list li').forEach(item => {
        item.addEventListener('mouseenter', event => {
            // Hide all course lists
            document.querySelectorAll('.course-list').forEach(courseList => {
                courseList.classList.remove('d-block');
                courseList.classList.add('d-none');
            });

            // Remove active class from all links
            document.querySelectorAll('.category-submenu-list-heading').forEach(link => {
                link.classList.remove('active');
            });

            // Show the targeted course list
            const target = event.target.getAttribute('data-target');
            document.getElementById(target).classList.remove('d-none');
            document.getElementById(target).classList.add('d-block');

            // Add active class to the hovered link
            event.target.classList.add('active');
        });
    });

    // Show dropdown on hover
    allCoursesButton.addEventListener('mouseenter', function() {
        dropdownContainer.style.display = 'block';
    });

    // Hide dropdown when mouse leaves the button or dropdown
    allCoursesButton.addEventListener('mouseleave', function() {
        setTimeout(() => {
            if (!dropdownContainer.matches(':hover')) {
                dropdownContainer.style.display = 'none';
            }
        }, 100);
    });

    dropdownContainer.addEventListener('mouseleave', function() {
        dropdownContainer.style.display = 'none';
    });

    dropdownContainer.addEventListener('mouseenter', function() {
        dropdownContainer.style.display = 'block';
    });
});


</script>


