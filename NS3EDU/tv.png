<script>
    $(document).ready(function() {
        $('#contactForm').submit(function(event) {
            $('#country_code').val('+' + $("#mobile_code").intlTelInput("getSelectedCountryData").dialCode);
            if (!this.checkValidity()) {
                event.preventDefault();
                event.stopPropagation();
            }
            this.classList.add('was-validated');
        });

        // Initialize Bootstrap tooltips
        $('[data-toggle="tooltip"]').tooltip();

        // Refresh CAPTCHA code
        $('#refreshCaptcha').on('click', function() {
            // Regenerate CAPTCHA code
            var captchaChars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
            var newCaptcha = '';
            for (var i = 0; i < 6; i++) {
                newCaptcha += captchaChars.charAt(Math.floor(Math.random() * captchaChars.length));
            }
            $('#captchaCode').text(newCaptcha); // Update the displayed CAPTCHA code
        });


    });
    // Country Code Selection
    $("#mobile_code").intlTelInput({
        preferredCountries: ["in", "us", "au", "ca", "gb"],
        separateDialCode: true,
    });
    // Capture selected country code
    $('#mobile_code').on('countrychange', function(e, countryData) {
        $('#country_code').val('+' + countryData.dialCode);
    });
</script>

<script>
    document.addEventListener("DOMContentLoaded", function() {
  const imgContainer = document.querySelector('.img-container');
  const highlightsWrapper = document.querySelector('.highlights-wrapper');
  
  // Check if browser supports position: sticky
  const testElement = document.createElement('div');
  testElement.style.position = 'sticky';
  testElement.style.position = '-webkit-sticky'; // For Safari
  if (testElement.style.position === 'sticky' || testElement.style.position === '-webkit-sticky') {
    // Browser supports position: sticky
    return;
  }

  // Fallback for older browsers
  window.addEventListener('scroll', function() {
    const rect = highlightsWrapper.getBoundingClientRect();
    if (rect.top <= 112 && rect.bottom > 112) {
      imgContainer.classList.add('fixed-certificate');
    } else {
      imgContainer.classList.remove('fixed-certificate');
    }
  });
});

</script>

